class Controler extends EventTarget {
    constructor(){
        super();
        JSON.parse(localStorage["user"])=false;
        console.log(this.user)
    }

    addOglas(event){
        this.dispatchEvent(
            new CustomEvent(
                "newOglas",
                {detail: {oglas: event}}
                )
        );

    }
}
let controler=new Controler
module.exports = controler;