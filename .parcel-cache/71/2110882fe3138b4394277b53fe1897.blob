.tabs {
  .tab {
    a {
      color: $custom-color;
      &.active {color: $custom-color;}
      &:hover {color: $custom-color;}
      &:focus {
        background-color: rgba($custom-color, 0.1);
        &.active{
					background-color: rgba($custom-color, 0.1);
				}
      }
    }
  }
  .indicator {background-color: $custom-color;}
}